{"ast":null,"code":"import LoginForm from '@/components/LoginForm.vue';\nimport SettingTools from '@/components/SettingTools/SettingTools.vue';\nimport HeadTop from '@/components/HeadTop/HeadTop.vue';\nexport default {\n  name: 'LoginPage',\n  filters: {\n    capitalize: function capitalize(value) {\n      return value + '登录';\n    }\n  },\n  props: {\n    msg: String\n  },\n  data: function data() {\n    return {\n      styleData: {},\n      logo_path: '',\n      footerData: [],\n      open: false,\n      param: {\n        username: '',\n        password: ''\n      },\n      rules: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }]\n      },\n      form: {\n        settingData: ''\n      },\n      isAdmin: true,\n      title: '',\n      pubKey: '',\n      EMAIL_REGEX: /^(?=.{1,254}$)(?=.{1,64}@)[a-zA-Z0-9!#$%&'*+/=?^_`{|}~-]+(?:\\.[a-zA-Z0-9!#$%&'*+/=?^_`{|}~-]+)*@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$/,\n      MOBILENUM_REGEX: /^\\d{4,11}$/,\n      changeWidth: 0,\n      loginWays: [],\n      footDatas: {},\n      allRight: false,\n      // 背景图片\n      Imgs: [],\n      imgObj: {},\n      // 所有的数据\n      allData: {},\n      // icon图标路径\n      icon_path: '',\n      // ip api url地址\n      ipAddress: ''\n    };\n  },\n  created: function created() {},\n  beforeUpdate: function beforeUpdate() {\n    this.setIcon();\n  },\n  computed: {\n    aroundNum: function aroundNum() {\n      return Math.floor(Math.random() * this.Imgs.length);\n    }\n  },\n  methods: {\n    hiddenRight: function hiddenRight() {\n      this.$refs.child.closeRight(); // console.log('asdfa');\n    },\n    closeB: function closeB(res) {\n      this.changeWidth = res;\n    },\n    openB: function openB(res) {\n      this.changeWidth = res;\n    },\n    //----------可优化-----------\n    getpubkey: function getpubkey(res) {\n      this.pubKey = res;\n    },\n    getNewImg: function getNewImg(res) {\n      this.imgObj = res;\n    },\n    getImgs: function getImgs(res) {\n      this.Imgs = res;\n    },\n    getway: function getway(res) {\n      this.loginWays = res;\n    },\n    getTopData: function getTopData(res) {\n      this.footDatas = res;\n    },\n    getBlu: function getBlu(res) {\n      this.allRight = res;\n    },\n    getAllData: function getAllData(res) {\n      this.styleData = res.styleData;\n      this.footerData = res.footerData;\n      this.loginWays = res.loginWays;\n      this.footDatas = res.footData;\n      this.allData = res;\n    },\n    getlogo: function getlogo(res) {\n      this.logo_path = res;\n    },\n    geticon: function geticon(res) {\n      console.log(res);\n      this.icon_path = res;\n    },\n    // icon\n    setIcon: function setIcon() {\n      document.querySelector('link').href = this.icon_path;\n    }\n  },\n  mounted: function mounted() {},\n  components: {\n    LoginForm: LoginForm,\n    SettingTools: SettingTools,\n    HeadTop: HeadTop\n  }\n};","map":{"version":3,"mappings":"AAkGA;AACA;AACA;AACA;EACAA,iBADA;EAEAC;IACAC;MACA;IACA;EAHA,CAFA;EAOAC;IACAC;EADA,CAPA;EAUAC,IAVA,kBAUA;IACA;MACAC,aADA;MAEAC,aAFA;MAGAC,cAHA;MAIAC,WAJA;MAKAC;QACAC,YADA;QAEAC;MAFA,CALA;MASAC;QACAF;UAAAG;UAAAC;UAAAC;QAAA,EADA;QAEAJ;UAAAE;UAAAC;UAAAC;QAAA;MAFA,CATA;MAaAC;QACAC;MADA,CAbA;MAgBAC,aAhBA;MAiBAC,SAjBA;MAkBAC,UAlBA;MAmBAC,aACA,oMApBA;MAqBAC,6BArBA;MAsBAC,cAtBA;MAuBAC,aAvBA;MAwBAC,aAxBA;MAyBAC,eAzBA;MA0BA;MACAC,QA3BA;MA4BAC,UA5BA;MA6BA;MACAC,WA9BA;MA+BA;MACAC,aAhCA;MAiCA;MACAC;IAlCA;EAoCA,CA/CA;EAgDAC,OAhDA,qBAgDA,EAhDA;EAiDAC,YAjDA,0BAiDA;IACA;EACA,CAnDA;EAoDAC;IACAC;MACA;IACA;EAHA,CApDA;EAyDAC;IACAC,WADA,yBACA;MACA,8BADA,CAEA;IACA,CAJA;IAKAC;MACA;IACA,CAPA;IAQAC;MACA;IACA,CAVA;IAWA;IACAC,SAZA,qBAYAC,GAZA,EAYA;MACA;IACA,CAdA;IAeAC,SAfA,qBAeAD,GAfA,EAeA;MACA;IACA,CAjBA;IAkBAE,OAlBA,mBAkBAF,GAlBA,EAkBA;MACA;IACA,CApBA;IAqBAG,MArBA,kBAqBAH,GArBA,EAqBA;MACA;IACA,CAvBA;IAwBAI,UAxBA,sBAwBAJ,GAxBA,EAwBA;MACA;IACA,CA1BA;IA2BAK,MA3BA,kBA2BAL,GA3BA,EA2BA;MACA;IACA,CA7BA;IA8BAM,UA9BA,sBA8BAN,GA9BA,EA8BA;MACA;MACA;MACA;MACA;MAEA;IACA,CArCA;IAsCAO,OAtCA,mBAsCAP,GAtCA,EAsCA;MACA;IACA,CAxCA;IAyCAQ,OAzCA,mBAyCAR,GAzCA,EAyCA;MACAS;MACA;IACA,CA5CA;IA6CA;IACAC,OA9CA,qBA8CA;MACAC;IACA;EAhDA,CAzDA;EA2GAC,OA3GA,qBA2GA,EA3GA;EA4GAC;IACAC,oBADA;IAEAC,0BAFA;IAGAC;EAHA;AA5GA","names":["name","filters","capitalize","props","msg","data","styleData","logo_path","footerData","open","param","username","password","rules","required","message","trigger","form","settingData","isAdmin","title","pubKey","EMAIL_REGEX","MOBILENUM_REGEX","changeWidth","loginWays","footDatas","allRight","Imgs","imgObj","allData","icon_path","ipAddress","created","beforeUpdate","computed","aroundNum","methods","hiddenRight","closeB","openB","getpubkey","res","getNewImg","getImgs","getway","getTopData","getBlu","getAllData","getlogo","geticon","console","setIcon","document","mounted","components","LoginForm","SettingTools","HeadTop"],"sourceRoot":"src/views/LoginPage","sources":["LoginPageAdmin.vue"],"sourcesContent":["<template>\n    <div>\n        <div\n            :class=\"styleData.qf_homeStyle == 'allView' ? 'everyDayRate login-wrap' : 'everyDayRate-lr login-wrap-lr'\"\n            :style=\"{\n                backgroundImage:\n                    styleData.qf_homeStyle == 'allView' ? `url(${imgObj.flag == false ? Imgs[Imgs.length - 1] : Imgs[aroundNum]})` : 'none',\n                width: `${100 - changeWidth}%`\n            }\"\n            @click.stop=\"hiddenRight\"\n        >\n            <!-- 登录框的位置 -->\n            <div :class=\"styleData.qf_homeStyle == 'allView' ? styleData.qf_login : 'right-lr'\" id=\"mobile_form\">\n                <div :class=\"styleData.qf_homeStyle == 'allView' ? 'left-box' : 'left-box-lr'\">\n                    <!-- 在这里展示轮播图 -->\n                    <div v-show=\"styleData.qf_homeStyle == 'leftAndright'\" id=\"swiper-out-box\">\n                        <el-carousel\n                            :direction=\"styleData.qf_swiper_way\"\n                            :autoplay=\"styleData.qf_loopvalue\"\n                            :loop=\"true\"\n                            arrow=\"never\"\n                            :interval=\"3500\"\n                            :indicator-position=\"styleData.qf_swiper_pages\"\n                        >\n                            <el-carousel-item v-for=\"(item, index) in Imgs\" :key=\"index\">\n                                <img :src=\"item\" alt=\"swipermy-imgs\" class=\"swipermy-imgs\" />\n                            </el-carousel-item>\n                        </el-carousel>\n                    </div>\n                </div>\n                <div :class=\"styleData.qf_homeStyle == 'allView' ? 'right-box' : 'right-box-lr'\">\n                    <div class=\"inner-box\">\n                        <LoginForm :allObj=\"allData\" :styleData=\"styleData\" :logo_path=\"logo_path\" :isAdmin=\"isAdmin\" />\n                    </div>\n                </div>\n\n                <div class=\"setting-right\">\n                    <SettingTools\n                        ref=\"child\"\n                        @openBox=\"openB\"\n                        @closeBox=\"closeB\"\n                        @pullData=\"getAllData\"\n                        @loginwaycheck=\"getway\"\n                        @footArea=\"getTopData\"\n                        @sendImgs=\"getImgs\"\n                        @allright=\"getBlu\"\n                        @theNewImg=\"getNewImg\"\n                        @logo=\"getlogo\"\n                        @pubkey=\"getpubkey\"\n                        @icon=\"geticon\"\n                    ></SettingTools>\n                </div>\n            </div>\n\n            <!-- footer的位置 -->\n            <div class=\"footer-bottom\" v-if=\"styleData.login_footer == 1 && styleData.qf_homeStyle === 'allView'\">\n                <div class=\"footer-bottom-center\">\n                    <div class=\"move-box\">\n                        <div\n                            class=\"top-area\"\n                            :class=\"\n                                styleData.qf_footer == 'footer-bottom-center-up' && allRight == false ? styleData.qf_footer : 'top-area'\n                            \"\n                        >\n                            <div class=\"footer-item\" v-for=\"(item, index) in footDatas.topArea\" :key=\"index\">\n                                <a :href=\"item.href\">\n                                    <img v-if=\"item.img\" :src=\"item.img\" alt=\"\" width=\"20px\" height=\"20px\" />\n                                    <img onerror=\"this.style.display='none'\" src=\"#\" alt=\"\" />\n                                    <p style=\"float: right; height: 20px; line-height: 20px; margin: 0px 0px 0px 5px; color: #fff\">\n                                        {{ item.text }}\n                                    </p>\n                                </a>\n                            </div>\n                        </div>\n                        <div\n                            class=\"under-area\"\n                            :class=\"\n                                styleData.qf_footer == 'footer-bottom-center-down' && allRight == false ? styleData.qf_footer : 'under-area'\n                            \"\n                        >\n                            <div class=\"footer-item\" v-for=\"(item, index) in footDatas.underArea\" :key=\"index\">\n                                <a :href=\"item.href\">\n                                    <img v-if=\"item.img\" :src=\"item.img\" alt=\"\" width=\"20px\" height=\"20px\" />\n                                    <img onerror=\"this.style.display='none'\" src=\"#\" alt=\"\" />\n                                    <p style=\"float: right; height: 20px; line-height: 20px; margin: 0px 0px 0px 5px; color: #fff\">\n                                        {{ item.text }}\n                                    </p>\n                                </a>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport LoginForm from '@/components/LoginForm.vue';\nimport SettingTools from '@/components/SettingTools/SettingTools.vue';\nimport HeadTop from '@/components/HeadTop/HeadTop.vue';\nexport default {\n    name: 'LoginPage',\n    filters: {\n        capitalize: function (value) {\n            return value + '登录';\n        }\n    },\n    props: {\n        msg: String\n    },\n    data() {\n        return {\n            styleData: {},\n            logo_path: '',\n            footerData: [],\n            open: false,\n            param: {\n                username: '',\n                password: ''\n            },\n            rules: {\n                username: [{ required: true, message: '请输入用户名', trigger: 'blur' }],\n                password: [{ required: true, message: '请输入密码', trigger: 'blur' }]\n            },\n            form: {\n                settingData: ''\n            },\n            isAdmin: true,\n            title: '',\n            pubKey: '',\n            EMAIL_REGEX:\n                /^(?=.{1,254}$)(?=.{1,64}@)[a-zA-Z0-9!#$%&'*+/=?^_`{|}~-]+(?:\\.[a-zA-Z0-9!#$%&'*+/=?^_`{|}~-]+)*@[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(?:\\.[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*$/,\n            MOBILENUM_REGEX: /^\\d{4,11}$/,\n            changeWidth: 0,\n            loginWays: [],\n            footDatas: {},\n            allRight: false,\n            // 背景图片\n            Imgs: [],\n            imgObj: {},\n            // 所有的数据\n            allData: {},\n            // icon图标路径\n            icon_path: '',\n            // ip api url地址\n            ipAddress: ''\n        };\n    },\n    created() {},\n    beforeUpdate() {\n        this.setIcon();\n    },\n    computed: {\n        aroundNum: function () {\n            return Math.floor(Math.random() * this.Imgs.length);\n        }\n    },\n    methods: {\n        hiddenRight() {\n            this.$refs.child.closeRight();\n            // console.log('asdfa');\n        },\n        closeB: function (res) {\n            this.changeWidth = res;\n        },\n        openB: function (res) {\n            this.changeWidth = res;\n        },\n        //----------可优化-----------\n        getpubkey(res) {\n            this.pubKey = res;\n        },\n        getNewImg(res) {\n            this.imgObj = res;\n        },\n        getImgs(res) {\n            this.Imgs = res;\n        },\n        getway(res) {\n            this.loginWays = res;\n        },\n        getTopData(res) {\n            this.footDatas = res;\n        },\n        getBlu(res) {\n            this.allRight = res;\n        },\n        getAllData(res) {\n            this.styleData = res.styleData;\n            this.footerData = res.footerData;\n            this.loginWays = res.loginWays;\n            this.footDatas = res.footData;\n\n            this.allData = res;\n        },\n        getlogo(res) {\n            this.logo_path = res;\n        },\n        geticon(res) {\n            console.log(res);\n            this.icon_path = res;\n        },\n        // icon\n        setIcon() {\n            document.querySelector('link').href = this.icon_path;\n        }\n    },\n    mounted() {},\n    components: {\n        LoginForm,\n        SettingTools,\n        HeadTop\n    }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n@import './index.scss';\n</style>\n"]},"metadata":{},"sourceType":"module"}